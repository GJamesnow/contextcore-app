generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model AssetAnalysis {
  id               Int      @id @default(autoincrement())
  createdAt        DateTime @default(now())
  
  // Location
  country          String
  region           String
  city             String
  notes            String

  // Financials
  purchasePrice    Float
  renovationBudget Float
  grossRent        Float
  operatingExpenses Float

  // Metrics
  capRate          Float
  cashOnCash       Float
  contextScore     Int
}